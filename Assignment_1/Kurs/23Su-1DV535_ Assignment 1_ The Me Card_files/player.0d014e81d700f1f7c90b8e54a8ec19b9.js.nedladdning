:null,"backdrop_options":null,"author_template":null,"author":{"changes":[],"template":null,"position":null},"animations":null,"triggers":[],"actions":[],"screenshots":[]},{"title":{"sv":"Begränsa tillgänglighet","en":"Restrict"},"content":{"sv":"\u003ch2\u003eBegr\u0026auml;nsa \u0026aring;tkomst\u003c/h2\u003e\nBegr\u0026auml;nsa \u0026aring;tkomst\u0026nbsp;\u0026auml;r en \u0026aring;terkommande inst\u0026auml;llning f\u0026ouml;r aktiviteter och resurser i MyMoodle och kan l\u0026auml;ggas till i efterhand n\u0026auml;r du skapat Podcasten.\u003cbr /\u003e\n\u003cbr /\u003e\nDu kan begr\u0026auml;nsa tillg\u0026auml;ngligheten till Podcasten med villkor, exempelvis att studenten ska ha n\u0026aring;tt upp till ett visst resultat i en test eller att ett visst datum har intr\u0026auml;ffat.","en":"\u003ch2\u003eRestrict access\u003cstrong\u003e\u003c/strong\u003e\u003c/h2\u003e\n\u003cstrong\u003eRestrict access \u003c/strong\u003eis not discussed here. It is a recurring setting for MyMoodle activities and resources and can be added afterwards when you have created the Podcast.\u003cbr /\u003e\n\u003cbr /\u003e\nYou can limit accessibility to the Podcast with additional terms, for example, that the student should have reached a specified result in a test or after a specific date."},"path":null,"element":"#id_availabilityconditionsheader,div.moodle-dialogue-focused .moodle-dialogue-wrap","point_at_first":false,"position":"left","delay":null,"width":300,"z_index":null,"backdrop":true,"backdrop_cover":false,"final_step":false,"max_continuation_attempts":3,"class_names":["hide_title","hide_progress","hide_end","inmplayer_tpl_step_left_side"],"custom_data":{},"on_before_activate":"","on_activate":"","on_deactivate":"","on_show":"","author_changes":[],"display_popover":null,"persistent":false,"assets":[],"context":null,"step_version":2,"condition":null,"launcher":null,"url_trigger":null,"backdrop_options":null,"author_template":null,"author":{"changes":[],"template":nu